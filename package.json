    "vuetify": "^3.1.3"
  },
  "devDependencies": {
    "@nuxtjs/supabase": "^0.3.0",
    "@nuxtjs/svg": "^0.4.0",
    "@pinia/nuxt": "^0.4.6",
    "@types/luxon": "^3.2.0",
    "@types/uniqid": "^5.3.2",
    "@vueuse/nuxt": "^9.10.0",
    "nuxt": "3.0.0",
