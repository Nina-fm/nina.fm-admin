    "@vueuse/nuxt": "^9.10.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "html-entities": "^2.3.3",
    "nuxt": "^3.2.3",
    "pinia": "^2.0.28",
    "prettier": "^2.8.4",
    "query-string": "^8.1.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.4",
    "typescript-deno-plugin": "^1.31.0",
    "vite-plugin-vuetify": "^1.0.2"
