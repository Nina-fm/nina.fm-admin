    "mime-types": "^2.1.35",
    "sortablejs": "^1.15.0",
    "sortablejs-vue3": "^1.2.8",
    "vue-easy-lightbox": "^1.12.0",
    "vue3-dropzone": "^2.0.1",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.1.12"
  },
  "devDependencies": {
    "@nuxtjs/supabase": "^0.3.1",
    "@nuxtjs/svg": "^0.4.0",
    "@pinia/nuxt": "^0.4.6",
    "@types/lodash.debounce": "^4.0.7",
    "@types/luxon": "^3.2.0",
